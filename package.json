{
  "name": "@beforeyoubid/serverless-step-functions-offline",
  "version": "2.5.0",
  "description": "Serverlesss plugin to support step function offline",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/beforeyoubid/serverless-step-functions-offline/issues"
  },
  "homepage": "https://github.com/beforeyoubid/serverless-step-functions-offline#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:beforeyoubid/serverless-step-functions-offline.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' --quiet",
    "ts": "tsc --noEmit",
    "build-and-publish": "npm run build && npm publish --access public",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/*.ts --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "2.29.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.25",
    "@types/serverless": "1.78.33",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "asl-types": "^1.2.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "pre-commit": "^1.1.3",
    "prettier": "^2.2.1",
    "serverless": "2.53.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "step functions",
    "serverless-step-functions-offline"
  ],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ]
}
